<?xml version="1.0"?>
<config>
	<modules>
		<Biinno_Api>
			<version>0.1.0</version>
		</Biinno_Api>
	</modules>
	<global>
		<models>
            <api>
                <class>Biinno_Api_Model</class>
				<rewrite>
					<importzp>Biinno_Api_Model_Importer</importzp>
				</rewrite>
				<rewrite>
					<oppnotexist>Biinno_Api_Model_Optionproductnotexist</oppnotexist>
				</rewrite>
				<rewrite>
					<oppexist>Biinno_Api_Model_Optionproductexist</oppexist>
				</rewrite>
				<rewrite>
					<opstore>Biinno_Api_Model_Optionstore</opstore>
				</rewrite>
                                <rewrite>
					<show-previews-in-option>Biinno_Api_Model_ShowPreviewsInOption</show-previews-in-option>
				</rewrite>
            </api>			
        </models>
		<helpers>
			<api>
			   <class>Biinno_Api_Helper</class>
		    </api>
		</helpers>
		<events>
			<checkout_type_onepage_save_order_after>
			<!--checkout_onepage_controller_success_action-->
				<observers>
					<biinno_api_observer>
						<type>model</type>
						<class>api/observer</class>
						<method>exportOnOrderEvent</method>
					</biinno_api_observer>
				</observers>
			<!--/checkout_onepage_controller_success_action-->
			</checkout_type_onepage_save_order_after>
		</events>
	</global>
	<adminhtml>
			<menu></menu>
			<events></events>
			<translate></translate>
			<acl>
				<resources>
					<admin>
						<children>
							<system>
								<children>
									<config>
										<children>
											<api>
												<title>W2p Integration</title>
											</api>
										</children>
									</config>
								</children>
							</system>
						</children>
					</admin>
				</resources>
			</acl>
	</adminhtml>
</config>
