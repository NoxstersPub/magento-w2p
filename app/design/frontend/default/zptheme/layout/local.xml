<layout version="0.1.0">
  <catalog_product_view>
    <reference name="head">
      <action method="addItem"><type>skin_js</type><name>js/jquery-1.4.2.min.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/jquery-ui-1.8.custom.min.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/colorpicker.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/jquery-qtip-1.0.0-rc3.min.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/jquery.fancybox-1.3.1.pack.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/ajaxupload.js</name></action>
      <action method="addItem"><type>skin_js</type><name>js/zp-personalization-form.js</name></action>


      <action method="addItem"><type>skin_css</type><name>css/colorpicker.css</name></action>
      <action method="addItem"><type>skin_css</type><name>css/jquery.fancybox-1.3.1.css</name></action>
      <action method="addItem"><type>skin_css</type><name>css/zp-style.css</name></action>
    </reference>
  </catalog_product_view>

  <zetaprints_webtoprint_image_index>
    <reference name="root">
      <block type="page/html" name="root" output="toHtml" template="image-edit/edit.phtml" />
    </reference>
  </zetaprints_webtoprint_image_index>
</layout>
